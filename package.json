{
  "name": "yapi-ts-gen",
  "version": "1.0.5",
  "description": "YAPI TypeScript API Generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "yapi-ts-gen": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/songzhanpeng/yapi-ts-gen"
  },
